<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/l1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:orientation="vertical"
    tools:context=".login.view.ActivityLogin">

    <LinearLayout
        android:id="@+id/lyMain"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:background="@drawable/back_corner"
        android:gravity="center"
        android:orientation="vertical"
        tools:ignore="InvalidId">

        <ImageView
            android:id="@+id/vectorImageAccpCancl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textDialogAccpCancl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="13dp"
            android:textColor="@color/greenCorrect"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/lyChildreOne"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/buttonCancelDialogAccpCancl"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:background="@drawable/buttoncancel"
            android:text="@string/user_cancel"
            android:textColor="@color/white"
            android:textSize="15.4sp"
            android:textStyle="bold" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/buttonAcceptDialogAccpCancl"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/buttonerror"
            android:text="@string/user_accept"
            android:textColor="@color/white"
            android:textSize="15.4sp"
            android:textStyle="bold" />
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>
