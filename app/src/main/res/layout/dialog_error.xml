<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/l1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:orientation="vertical"
    tools:context=".login.view.ActivityLogin">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:background="@drawable/back_corner"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/vectorImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textDialogError"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="13dp"
            android:textColor="@color/greenCorrect"
            android:textStyle="bold" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/buttonAcceptDialog"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:background="@drawable/buttonerror"
            android:text="@string/user_accept"
            android:textColor="@color/white"
            android:textSize="15.4sp"
            android:textStyle="bold" />

    </LinearLayout>

</RelativeLayout>
